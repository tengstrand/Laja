
#macro fileContent()
  #package()
  #importBegin()
  #addImport("import java.util.List;")
  #importCertificate()
  #importEnd()
  #header()
public interface {stateListClass} extends List<{stateClass}> {
    Certificate certificate();
    boolean isValid();
    void encapsulate(Object encapsulator);
    void throwExceptionIfNotEncapsulatedBy(Object encapsulator);
}
#end

#set (filename = "{outputDir}/{stateListClass}.java")
#generateFile(filename, fileContent())
