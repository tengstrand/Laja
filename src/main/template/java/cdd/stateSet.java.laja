
#macro fileContent()
  #package()
import java.util.Set;
import {certificatePackage}.Certificate;

  #header()
public interface {stateSetClass} extends Set<{stateClass}> {
    Certificate certificate();
    boolean isValid();
    void encapsulate(Object encapsulator);
    void throwExceptionIfNotEncapsulatedBy(Object encapsulator);
}
#end

#set (filename = "{outputDir}/{stateSetClass}.java")
#generateFile(filename, fileContent())
